{
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017",
            "esnext.asynciterable"
        ],
        "module": "commonjs",
        "noImplicitAny": false,
        "noLib": false,
        "noStrictGenericChecks": true,
        "noUnusedLocals": false,
        "removeComments": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "target": "ES2017"
    }
}
